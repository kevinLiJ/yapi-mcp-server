#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules/parcel/lib/node_modules:/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules/parcel/node_modules:/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules:/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules/parcel/lib/node_modules:/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules/parcel/node_modules:/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules:/Users/bytedance/code/yapi-mcp-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules/parcel/lib/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/parcel@2.15.1_@swc+helpers@0.5.17/node_modules/parcel/lib/bin.js" "$@"
fi
